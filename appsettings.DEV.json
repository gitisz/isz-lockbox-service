{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Aws": {
    "ProxyHost": "",
    "ProxyPort": "",
    "BypassList": [
      "localhost"
    ],
    "Pofile": "",
    "Region": "",
    "AccountID": "",
    "EndpointDefinition": "http://localhost:8000"
  },
  "Keycloak": {
    "ServerRealm": "http://localhost:8080/realms/isz-realm/protocol/openid-connect/auth",
    "Metadata": "http://localhost:8080/realms/isz-realm/.well-known/uma2-configuration",
    "ClientId": "3bef7e68-ad77-4ddd-9510-5f829f048c48",
    "ClientSecret": "3bef7e68-ad77-4ddd-9510-5f829f048c48",
    "TokenExchange": "http://localhost:8080/realms/isz-realm/protocol/openid-connect/token",
    "Audience": "isz-trusted-audience"
